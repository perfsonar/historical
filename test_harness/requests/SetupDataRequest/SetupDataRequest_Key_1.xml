<?xml version="1.0" encoding="UTF-8"?>

<!-- 
Name:     SetupDataRequest_Key_1.xml

Purpose:  Test the 'SetupDataRequest' Message by sending a key element that
          should match something in the backend storage.  The results will 
          be actual data.
          
Varients: 
          - Send a correct vs. incorrect key to test performance.
          - Send multiple key elements to test correctness/performance.
          
Caveots:  Information in the key will vary from instance to instance:
          re-enter according to the storage of a particular deployment.

Results:  
          - Correct:   A data blocks filled with datum elements represting
                       the contents of the backend storage.
          - Incorrect: A result datum with an error condition for a given
                       metadata.
                       
          Note that in a single message it will be possible to have error
          conditions and success conditions for each metadata/data pair.  

Version:  $Id:$
-->

<nmwg:message type="SetupDataRequest" id="setupDataRequest1"
              xmlns:netutil="http://ggf.org/ns/nmwg/characteristic/utilization/2.0/"
              xmlns:nmwg="http://ggf.org/ns/nmwg/base/2.0/"
              xmlns:select="http://ggf.org/ns/nmwg/ops/select/2.0/"
              xmlns:nmwgt="http://ggf.org/ns/nmwg/topology/2.0/"
              xmlns:nmtm="http://ggf.org/ns/nmwg/time/2.0/">

  <nmwg:metadata id="m1" xmlns:nmwg="http://ggf.org/ns/nmwg/base/2.0/">
    <nmwg:key id="k1" xmlns:nmwg="http://ggf.org/ns/nmwg/base/2.0/">
      <nmwg:parameters id="pk1">
        <nmwg:parameter name="startTime">1121472000</nmwg:parameter>
        <nmwg:parameter name="endTime">1121472300</nmwg:parameter>
        <nmwg:parameter name="maKey">a63b47372a944d040d41d9fefdab7082</nmwg:parameter>
      </nmwg:parameters>
    </nmwg:key>
  </nmwg:metadata> 
  
  <nmwg:data id="d1" metadataIdRef="m1" xmlns:nmwg="http://ggf.org/ns/nmwg/base/2.0/"/>

</nmwg:message>
