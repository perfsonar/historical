<?xml version="1.0" encoding="UTF-8"?>
<!-- 
getAll: the name of the xml file containing the getAll request
requestDir: directory where the request files live (relative to $PSHOME)
input: the name of the xml file containing the test request
description: a description of the test
expected_element:  tags and attribute that you expect to find in the reloy
   For example:
     <expected_output>hostName</expected_output>
     <expected_output attrib="file">parameter</expected_output>

NOTE for MA testing: 
   You MUST run a test that returns a Key before a test that uses a Key.

Current Limitations (may fix these later):
  -You can't look for a tag that is part of the 'result' namespace 
     (http://ggf.org/ns/nmwg/result/2.0/). That is reserved for error/status messages
  -You can't look for specific element text or attribute values, only tag names and 
	attribute names
-->

<perfSONAR-tests>
  <requestDir>/test_harness/requests</requestDir>
  <getAll>SNMP/MetadataKeyRequest/MetadataKeyRequest_getAll.xml</getAll>
  <test>
     <input>EchoRequest/EchoRequest_1.xml</input>
     <description>Simple Echo Request Test </description>
     <expected_output>eventType</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MetadataKeyRequest_1.xml</input>
     <description>Find the metadata and Key for a given ifAddress</description>
     <expected_output>hostName</expected_output>
     <expected_output>ifName</expected_output>
     <expected_output>key</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MetadataKeyRequest_2.xml</input>
     <description>Find the metadata for a given ifAddress, consolidationFunction, etc.</description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
     <expected_output>hostName</expected_output>
     <expected_output>ifName</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MetadataKeyRequest_3.xml</input>
     <description>Find the metadata from a 'chain' of mergable elements</description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
     <expected_output>hostName</expected_output>
     <expected_output>ifName</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MetadataKeyRequest_4.xml</input>
     <description>Find the metadata from a 'chain' of filters </description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
     <expected_output>hostName</expected_output>
     <expected_output>ifName</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MetadataKeyRequest_Key_1.xml</input>
     <description>Find the metadata from a key </description>
     <expected_output attrib="name">parameter</expected_output>
     <expected_output>key</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MetadataKeyRequest_Key_2.xml</input>
     <description>Find the metadata from a key and a filter</description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>
 <test>
     <input>SNMP/MetadataKeyRequest/MDKR-key.xml</input>
     <description> Request metadata by key </description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MDKR-cooked_key-filter.xml</input>
     <description> Request metadata using a cooked key </description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MDKR-md-filter-merge.xml</input>
     <description> Request metadata using filter and merge chaining </description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MDKR-cooked_key.xml</input>
     <description> request metdata using a cooked key </description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MDKR-md.xml</input>
     <description> request metadata  </description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MDKR-md-filter.xml</input>
     <description> request metadata with filter </description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MDKR-md-merge.xml</input>
     <description> request metadata with merge </description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MDKR-key-filter.xml</input>
     <description>request metadata by Key and subject chaining.</description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>
  <test>
     <input>SNMP/MetadataKeyRequest/MDKR-key-filter-merge.xml</input>
     <description> request metadata by key with filter and merge</description>
     <expected_output>key</expected_output>
     <expected_output attrib="name">parameter</expected_output>
  </test>


