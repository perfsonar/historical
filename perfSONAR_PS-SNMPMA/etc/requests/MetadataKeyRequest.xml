        <nmwg:parameter name="maKey">a63b47372a944d040d41d9fefdab7082</nmwg:parameter>
      <nmwg:parameter name="startTime">1121472000</nmwg:parameter>
      <nmwg:parameter name="endTime">1121472600</nmwg:parameter>
