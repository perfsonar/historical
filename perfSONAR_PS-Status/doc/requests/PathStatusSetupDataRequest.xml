<?xml version="1.0" encoding="utf-8"?>
<nmwg:message type="SetupDataRequest" xmlns:nmwg="http://ggf.org/ns/nmwg/base/2.0/" xmlns:nmtl2="http://ggf.org/ns/nmwg/topology/l2/3.0/" xmlns:select="http://ggf.org/ns/nmwg/ops/select/2.0/" xmlns:nmwgtopo3="http://ggf.org/ns/nmwg/topology/base/3.0/">
