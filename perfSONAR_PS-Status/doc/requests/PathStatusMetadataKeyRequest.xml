<?xml version="1.0" encoding="utf-8"?>
<nmwg:message type="MetadataKeyRequest" xmlns:select="http://ggf.org/ns/nmwg/ops/select/2.0/" xmlns:nmwg="http://ggf.org/ns/nmwg/base/2.0/">
  <!-- This query returns keys that can be used to query for elements. This key will have
  a specific timeframe encoded into them so any future requests will be restricted to data
  in that timeframe. It is mostly useful when times are not specified as the keys can be
  used to return the most up to date status -->
  <nmwg:metadata id="meta1">
    <nmwg:eventType>Path.Status</nmwg:eventType>
  </nmwg:metadata>
  <nmwg:metadata id="meta2">
    <select:subject metadataIdRef="meta1" />
    <select:parameters>
      <select:parameter name="startTime">1233931638</select:parameter>
      <select:parameter name="endTime">1234033378</select:parameter>
    </select:parameters>
  </nmwg:metadata>
  <nmwg:data id="data1" metadataIdRef="meta2" />
</nmwg:message>
