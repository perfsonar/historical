<nmwg:message type="SetupDataRequest" 
            id="#req1"
            xmlns="http://ggf.org/ns/nmwg/tools/org/perfsonar/1.0/" 
            xmlns:nmwg="http://ggf.org/ns/nmwg/base/2.0/" 
            xmlns:topo="http://ggf.org/ns/nmwg/topology/2.0/"
            xmlns:pinger="http://ggf.org/ns/nmwg/tools/pinger/1.0/"> 


  <nmwg:metadata id="#meta1">
    <pinger:subject id="#subject" 
      <topo:endPointPair>
        <topo:src type="ipv4" value="iepm-bw.slac.stanford.edu"/>
        <topo:dst type="ipv4" value="atlang-hstnng.abilene.ucaid.edu"/>
      </topo:endPointPair>
    </pinger:subject>
    <pinger:parameters>
      <nmwg:parameter name="count">10</nmwg:parameter>
      <nmwg:parameter name="interval">1000</nmwg:parameter> <!-- milliseconds -->
      <nmwg:parameter name="packetSize'>64</nmwg:parameter> <!-- bytes --> 
    </pinger:parameters>
  </nmwg:metadata>

  <nmwg:data id="#data1"
            metadataIdRef="#meta1"/>

</nmwg:message>
